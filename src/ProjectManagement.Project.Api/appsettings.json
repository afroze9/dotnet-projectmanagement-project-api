{
  "$schema": "https://steeltoe.io/schema/latest/schema.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Consul": {
    "Discovery": {
      "ServiceName": "project-api",
      "Scheme": "https",
      "Retry": {
        "Enabled": true,
        "MaxAttempts": 5
      }
    }
  }
}
